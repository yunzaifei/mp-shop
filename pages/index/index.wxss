/**index.wxss**/
.sw-container {
  height: 340rpx;
}
.sw-image {
  width: 100%;
}
.cates-container {
  display: flex;
}
.cates-nv {
  flex: 1;
  padding: 20rpx;
}
.cates-image {
  width: 100%;
}
.floor-title {
  padding: 10rpx 0;
}
.floor-title image {
  width: 100%;
}
.floor-list {
  overflow: hidden;
}
.floor-list navigator {
  width: 33.33%;
  float: left;
}
.floor-list navigator:nth-last-child(-n+4) { 
  height: 27.72vw;
  width: calc(33.33% - 10rpx);
  border-left: 10rpx solid #fff;
}
.floor-list navigator:nth-child(2), .floor-list navigator:nth-child(3) { 
  height: calc(27.72vw - 10rpx);
  border-bottom: 10rpx solid #fff;
}
.floor-list image {
  width: 100%;
  height: 100%;
}